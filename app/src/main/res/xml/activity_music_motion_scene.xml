<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/view"
            motion:dragDirection="dragDown"
            motion:touchAnchorSide="bottom" />
        <OnSwipe
            motion:dragDirection="dragDown"
            motion:touchAnchorSide="bottom" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="0dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:id="@+id/view2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_editor_absoluteY="0dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="337dp"
            android:layout_height="371dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="116dp"
            android:id="@+id/textView6" />
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/view" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="110dp"
            android:layout_width="0dp"
            android:id="@+id/view"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:scaleX="1"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="20dp"
            android:layout_marginBottom="24dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="220dp"
            android:id="@+id/imageView3"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="46dp" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/imageView4"
            motion:layout_constraintBottom_toTopOf="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="@+id/imageView3"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="61dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintHorizontal_bias="0.051"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="99dp"
            android:id="@+id/imageView"
            motion:layout_constraintBottom_toBottomOf="@+id/view" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/imageView7"
            motion:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="20dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView6" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/imageView6"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            motion:layout_constraintTop_toTopOf="@+id/imageView5"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="30dp"
            android:id="@+id/imageView5"
            motion:layout_constraintStart_toEndOf="@+id/imageView4"
            motion:layout_constraintTop_toTopOf="@+id/imageView4"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="20dp" />
        <Constraint
            android:id="@+id/imageView9"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            android:layout_marginBottom="32dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:visibility="invisible" ></Constraint>
        <Constraint
            android:layout_height="0dp"
            motion:layout_constraintBottom_toTopOf="@+id/view"
            android:layout_width="0dp"
            android:id="@+id/view2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"></Constraint>
        <Constraint
            android:layout_height="371dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="116dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="337dp"
            android:id="@+id/textView6" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView12"
            motion:layout_constraintStart_toStartOf="@+id/view2"
            motion:layout_constraintEnd_toEndOf="@+id/view2"
            motion:layout_constraintTop_toTopOf="@+id/view2"
            android:layout_marginTop="68dp" />
    </ConstraintSet>
</MotionScene>